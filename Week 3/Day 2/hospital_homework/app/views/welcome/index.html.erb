<h1>St. Mary's Hospital</h1>
<br>
<%= link_to "Add a Patient", new_patient_path %>
<ul>
  <% @patients.each do |patient| %>
  <li>
    <%=link_to patient.name, patient_path(patient)%> :  
    <%=patient.gender%> : 
    <%=patient.birth%> : 
    <%=patient.diagnosis%> : 
    <%= link_to "Edit", edit_patient_path(patient.id) %> : 
    <%= link_to "Delete", patient_path(patient), method: :delete %>
  </li>
  <% end %>
</ul>